@article{Janssen2009573,
title = "A database for integrated assessment of European agricultural systems ",
journal = "Environmental Science & Policy ",
volume = "12",
number = "5",
pages = "573 - 587",
year = "2009",
note = "Integrated Assessment of Agricultural and Environmental Policies – concepts and tools ",
issn = "1462-9011",
doi = "http://dx.doi.org/10.1016/j.envsci.2009.01.007",
url = "http://www.sciencedirect.com/science/article/pii/S1462901109000203",
author = "Sander Janssen and Erling Andersen and Ioannis N. Athanasiadis and Martin K. van Ittersum",
keywords = "Policy making",
keywords = "Data and model integration",
keywords = "Database integration ",
abstract = "A major bottleneck for data-based policy making is that data sources are collected, managed, and distributed by different institutions, residing in different locations, resulting in conceptual and practical problems. The use of dispersed data for agricultural systems research requires the integration of data sources, which means to ensure consistency in data interpretations, units, spatial and temporal scales, to respect legal regulations of privacy, ownership and copyright, and to enable easy dissemination of data. This paper describes the \{SEAMLESS\} integrated database on European agricultural systems. It contains data on cropping patterns, production, farm structural data, soil and climate conditions, current agricultural management and policy information. To arrive at one integrated database, a shared ontology was developed according to a collaborative process, which facilitates interdisciplinary research. The paper details this process, which can be re-used in other research projects for integrating data sources. "
}

